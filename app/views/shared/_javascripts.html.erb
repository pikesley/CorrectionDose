<script>
$('.glucose-measurement-value .value').each(function() {
  var value = $(this).text().trim().split(' ')[0]
  if(value > 8.0) {
    $(this).parent().addClass('bg-high')
  }
  if(value < 4.0) {
    $(this).parent().addClass('bg-low')
  }
})

$(function () {
  $('.datetime').datetimepicker({
    format: 'YYYY-MM-DD HH:mm',
    defaultDate: moment()
  });
});
</script>
