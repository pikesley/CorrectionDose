<script>
$('.glucose-measurement-value').each(function() {
  if($(this).text() > 8.0) {
    $(this).addClass('bg-high')
  }
  if($(this).text() < 4.0) {
    $(this).addClass('bg-low')
  }
})

$(function () {
  $('.datetime').datetimepicker({
    format: 'YYYY-MM-DD HH:mm',
    defaultDate: moment()
  });
});
</script>
